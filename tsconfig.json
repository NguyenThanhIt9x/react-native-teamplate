{
  "compilerOptions": {
    "jsx": "react",
    "baseUrl": "./",
    "paths": {
      "@access/*": ["src/access/*"],
      "@constant/*": ["src/constant/*"],
      "@navigation/*": ["src/navigation/*"],
      "@redux/*": ["src/reduxs/*"],
      "@sagas/*": ["src/sagas/*"],
      "@screens/*": ["src/screens/*"],
      "@app/*": ["src/*"],
      "*": ["./*"],
    }
  },
  "extends": "@tsconfig/react-native/tsconfig.json",
  "exclude": ["__tests__/**/*-test.ts"]
}
